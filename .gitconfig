[user]
	name = Sho Kusano
	email = rosylilly@aduca.org
[alias]
  st = status -sb
  co = checkout
  ci = commit -v
  ca = ci -a
  bc = branch
  ps = push
  pl = pull
  pb = pull --rebase
  tracking = checkout -t
  io = !"sh -c 'curl -i http://git.io/ -F url=$1 `[ -n \"$2\" ] && echo -F code=$2`' -"
  pp = !"git pull --rebase && git push"
[core]
  editor = vim
[color]
  ui = true
[push]
  default = tracking
[tig "bind"]
  generic = g move-first-line
  generic = G move-last-line
[github]
	user = rosylilly
